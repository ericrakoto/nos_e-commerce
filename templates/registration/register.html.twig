<title>{% block title %}{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{asset('css/bootstrap.css')}}" type="text/css" >

    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModalCenter">
                      Midira ho mpikambana &raquo;
                    </button>
                    {% endblock %}

<div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Fisoratana anarana</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="border-radius: 50%;background-color: red;">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <!-- asiana zavatra -->
        <div>
    {{ form_start(registrationForm) }}
    {{ form_row(registrationForm.nom,{'label': 'Anarana', 'attr':{'placeholder': 'soraty eto ny anaranao...'}} )}}
    {{ form_row(registrationForm.prenom,{'label': 'Fanampiny', 'attr':{'placeholder': 'soraty eto ny anarana fanampiny...'}} )}}
    {{ form_row(registrationForm.tel,{'label': 'Laharana finday', 'attr':{'placeholder': 'soraty eto ny laharana finday...'}} )}}
    {{ form_row(registrationForm.adresse,{'label': 'Adiresy', 'attr':{'placeholder': 'soraty eto ny adiresinao...'}} )}}
    {{ form_row(registrationForm.email,{'label': 'Mailaka', 'attr':{'placeholder': 'soraty eto ny adiresy mailaka...'}} )}}
    {{ form_row(registrationForm.plainPassword,{'label': 'Teny miafina', 'attr':{'placeholder': 'soraty eto ny teny miafina...'}} )}}
    <button class="btn btn-outline-success" style="margin:10px;" type="submit">Enregistrer</button>
    {{ form_end(registrationForm) }}
    <div style="margin:10px;">
     </div>
    
      </div>
    </div>
  </div>
</div>
{% block javascripts %}
            <script src="{{asset('js/jquery-3.5.1.js')}}"></script>
             <script src="{{asset('js/bootstrap.bundle.js')}}"></script>
            <script src="{{asset('js/bootstrap.js')}}"></script>
            <script src="{{asset('js/glm-ajax.js')}}"></script>
            <script type="text/javascript">
                {% block js %}{% endblock %}
            </script>
      
{% endblock %}